{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "linebreak-style": "off",
    "no-trailing-spaces": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "eslint-disable-next-line no-console": ".js"
  }
}